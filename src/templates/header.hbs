<div class="header">
    <div class="sidebar">
        <div class="avatar">
            <div class="avatar-wrap">
                <img class="avatar__img" src="{{photoUrl}}"
                    alt="photo">
            </div>
        </div>
    </div>
    <div class="content">
        <h1 class="title-1">{{fullName}}</h1>
        <h3 class="title-2">Front-End Developer</h3>
    </div>
</div>
<ul class="contacts">   
    {{#each contacts}}
        {{> contact }}
    {{/each}}
</ul>