<div class="header">
    <div class="sidebar">
        <span class="photo">
            <img class="photo__img" src="{{photoUrl}}"
                alt="photo">
        </span>
    </div>
    <div class="content">
        <h1 class="title-1">{{fullName}}</h1>
        <h3 class="title-3">Front-End Developer</h3>
